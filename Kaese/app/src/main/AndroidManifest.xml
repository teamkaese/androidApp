<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackhb19.fawadjawaidmalik.teamkaese">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.FLASHLIGHT"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CheeseMap"></activity>
        <activity
            android:name=".MenuActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AddItemActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".FindItemActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".QRScannerActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".FoundProductActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".DeletePackageActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".StorageScanActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

    </application>

</manifest>